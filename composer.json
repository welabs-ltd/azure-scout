{
    "name": "welabs/azure-scout",
    "description": "Azure Search integration for Laravel Scout",
    "type": "library",
    "license": "MIT",
    "authors": [
        {
            "name": "Mahbub Rabbani",
            "email": "mahbub.rucse@gmail.com"
        }
    ],
    "autoload": {
        "psr-4": {
            "WeLabs\\AzureScout\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "WeLabs\\AzureScout\\Tests\\": "tests/"
        }
