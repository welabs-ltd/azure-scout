        "orchestra/testbench": "^8.0",
        "phpunit/phpunit": "^10.0"
    },
    "extra": {
        "laravel": {
            "providers": [
                "WeLabs\\AzureScout\\ServiceProvider"
            ]
        }
    },
    "minimum-stability": "dev"
} 